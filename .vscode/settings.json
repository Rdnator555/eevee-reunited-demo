{
	"cSpell.customDictionaries": {
		"custom-dictionary-workspace": {
			"name": "custom-dictionary-workspace",
			"path": "${workspaceFolder:eevee_reunited}/.cspell/custom-dictionary-workspace.txt",
			"addWords": true,
			"scope": "workspace"
		}
	},
	"Lua.completion.autoRequire": false,
	"Lua.runtime.special": {
		"include": "require"
	},
	"Lua.diagnostics.globals": [
		"CustomCoopGhost",
		"Poglite",
		"addCostumeToIgnoreList",
		"NoCostumes",
		"ModConfigMenu",
		"DeadSeaScrollsMenu",
		"Sewn_API",
		"UniqueItemsAPI"
	],
	"prettier.useTabs": true,
	"prettier.tabWidth": 4,
	"files.associations": {
		"*.anm2": "xml"
	}
}
